<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:lobi="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/lobi_icon_small"
    android:layout_height="@dimen/lobi_icon_small"
    >
    <com.kayac.lobi.libnakamap.components.ImageLoaderView
        android:id="@+id/lobi_account_thumbnail_image_loader"
        android:layout_width="@dimen/lobi_icon_small"
        android:layout_height="@dimen/lobi_icon_small"
        android:paddingLeft="1dp"
        android:paddingRight="1dp"
        android:paddingBottom="1dp"
        android:scaleType="fitXY"
        lobi:lobi_useMemoryCache="true"
        />
    <ImageView
        android:layout_width="@dimen/lobi_icon_small"
        android:layout_height="@dimen/lobi_icon_small"
        android:src="@drawable/lobi_frame_actionbar"
        />
</merge>