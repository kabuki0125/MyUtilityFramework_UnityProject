<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:lobi="http://schemas.android.com/apk/res-auto"
    >
    <com.kayac.lobi.libnakamap.components.ImageLoaderView
        android:id="@+id/lobi_framed_image_loader_loader"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        lobi:lobi_useMemoryCache="true"
        />
    <ImageView
        android:id="@+id/lobi_framed_image_loader_frame"
        android:src="@drawable/lobi_frame_s"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
    <ImageView 
        android:id="@+id/lobi_framed_image_loader_change_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/lobi_icn_btn_photo"
        android:visibility="gone"
        />
</merge>